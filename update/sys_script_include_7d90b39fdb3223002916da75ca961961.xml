	getEmailValidation: function(emailStr){
		emailStr = emailStr+'';
	
	if(emailStr.match(/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/)){
		return true;	
	}
	else{
		return false;
	}
	},
    type: 'NeedItEmailUtils'
};]]></script>
